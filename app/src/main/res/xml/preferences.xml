<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android"
       android:title="@string/myalarm">
    <CheckBoxPreference android:defaultValue="false"
        android:key="alarm_enabled"
        android:title="@string/pref_title_alarm_enabled"
        android:persistent="false">

    </CheckBoxPreference>
    <Preference android:title="@string/pref_title_alarm_time"
        android:key="alarm_time"
        android:summary="@string/pref_summary_default"
        android:persistent="false">

    </Preference>
    <Preference android:summary="@string/pref_summary_default"
        android:key="song"
        android:title="@string/pref_title_song"
        android:persistent="false">

    </Preference>
</PreferenceScreen>
